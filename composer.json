{
    "name": "yiithings/yii2-devkit",
    "description": "Developer helper kit for Yii2 framework",
    "type": "yii2-extension",
    "keywords": ["yii2","extension","developer","kit"],
    "license": "MIT",
    "authors": [
        {
            "name": "Panlatent",
            "email": "panlatent@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "yiisoft/yii2": "*",
        "iiifx-production/yii2-autocomplete-helper": "^1.3",
        "jamband/yii2-schemadump": "^0.10.0",
        "yiisoft/yii2-gii": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "yiithings\\devkit\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "composer/composer": "^1.6"
    },
    "autoload-dev": {
        "psr-4": {
            "tests\\": "tests/"
        }
    }
}
