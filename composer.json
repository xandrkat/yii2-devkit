{
    "name": "xandrkat/yii2-devkit",
    "description": "Developer helper kit for Yii2 framework",
    "type": "yii2-extension",
    "keywords": ["yii2","extension","developer","kit"],
    "license": "MIT",
    "authors": [
        {
            "name": "Alexandr Katrazhenko",
            "email": "katrazhenko@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "yiisoft/yii2": "*",
        "mitirrli/yii2-ide-helper": "*",
        "esempla/yii2tech-dump-migration": "*",
        "yiisoft/yii2-gii": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "yiithings\\devkit\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "composer/composer": "*"
    },
    "autoload-dev": {
        "psr-4": {
            "tests\\": "tests/"
        }
    }
}
